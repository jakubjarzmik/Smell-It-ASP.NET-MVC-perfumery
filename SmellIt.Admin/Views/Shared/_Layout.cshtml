<!DOCTYPE html>
<html>

<head>
    <partial name="PartialViews/LayoutParts/_Header" />
</head>

<body class="grey darken-2">
    <partial name="PartialViews/LayoutParts/_Navigation" />

    <main class="section">
        <div class="container">
            <div class="row">
                <div class="col s12">
                    <div class="card grey darken-3 white-text">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </main>

    <partial name="PartialViews/LayoutParts/_Footer" />

    <partial name="PartialViews/LayoutParts/_Scripts" />

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>